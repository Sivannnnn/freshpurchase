/* app.wxss */
@import "./style/colorUi/icon.wxss";
@import "./style/colorUi/main.wxss";
/* iconfont */
@import "./style/iconfont/iconfont.wxss";
/* common.css */
@import "./style/common.wxss";

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 

button {
  background: initial;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}

.clearfix::after {
  content: "";
  clear: both;
  display: block;
}

page {
  background-color: #FFF;
  height: 100%;
  overflow: hidden;
  --themeColor: #00BFFF;
}