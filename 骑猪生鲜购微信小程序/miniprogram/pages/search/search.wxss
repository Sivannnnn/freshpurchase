page {
  background-color: var(--themeColor);
}
.search {
  position: relative;
}
.search .search_row {
  margin: 0 auto;
  width: 80%;
  margin-top: 20rpx;
  height: 60rpx;
  position: relative;
}
.search .search_row .search_row_srh {
  position: absolute;
  font-size: 38rpx;
  left: 2%;
  top: 20%;
}
.search .search_row input {
  background: #fff;
  padding: 10rpx 120rpx 10rpx 55rpx;
  border-radius: 20rpx;
  font-size: 36rpx;
  width: 100%;
  height: 100%;
}
.search .search_row .search_row_clear {
  position: absolute;
  top: 25%;
  right: 15%;
  font-size: 28rpx;
}
.search .search_row .search_row_btn {
  position: absolute;
  top: 5%;
  right: 1%;
  border-left: 2rpx solid #808080;
  padding: 8rpx;
}
.search .search_toCart {
  position: absolute;
  right: 3%;
  top: 2%;
}
.search .search_toCart text {
  font-size: 45rpx;
}
.search_content {
  position: relative;
  background-color: #fff;
  margin-top: 40rpx;
  padding: 20rpx 40rpx;
  overflow-y: scroll;
  border-top-left-radius: 50rpx;
  border-top-right-radius: 50rpx;
}
.search_content .search_item {
  padding: 20rpx;
  color: #A9A9A9;
  font-size: 36rpx;
  border-bottom: 2rpx solid #A9A9A9;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.search_content .search_item .search_item_srh {
  font-size: 36rpx;
  margin-right: 20rpx;
}
.search_content .search_item:last-child {
  border-bottom: none;
}
.search_content .search_content_history_tit {
  font-size: 40rpx;
  font-weight: bold;
  margin-bottom: 20rpx;
}
.search_content .search_content_history_con {
  display: flex;
  flex-wrap: wrap;
}
.search_content .search_content_history_con navigator {
  margin: 15rpx;
  padding: 5rpx 10rpx;
  background-color: #D3D3D3;
  border-radius: 10rpx;
}
